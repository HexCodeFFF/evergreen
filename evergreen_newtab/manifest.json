{
  "manifest_version": 2,
  "name": "Evergreen New Tab [DEV]",
  "short_name": "Evergreen",
  "description": "A beautiful and minimalistic new tab extension for Chrome",
  "version": "1.1",
  "permissions": [
    "storage",
    "geolocation"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "icons": {
    "32": "evergreendev32.png",
    "128": "evergreendev128.png",
    "1000": "evergreendev1000.png"
  },
  "browser_action": {
    "default_icon": {
      "32": "evergreendev32.png",
      "128": "evergreendev128.png",
      "1000": "evergreendev1000.png"
    },
    "default_title": "Evergreen New Tab"
  },
  "content_security_policy": "script-src 'self' https://api.darksky.net/; object-src 'self';"
}